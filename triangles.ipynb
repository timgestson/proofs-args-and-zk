            "source": [
                "from util import flatten\n",
                "from felt import Felt\n",
                "from triangles import TriangleProver, TriangleVerifier",
            ],
        },
        {
            "cell_type": "code",
            "execution_count": 2,
            "metadata": {},
            "outputs": [],
            "source": [
                "class Graph(dict):\n",
                "    def __init__(self, graph):\n",
                "        super().__init__(graph)\n",
                "\n",
                "    def toAdjacencyMatrix(self):\n",
                "        n = len(self)\n",
                "        matrix = [[0] * n for _ in range(n)]\n",
                "        for i in range(n):\n",
                "            for j in range(n):\n",
                "                matrix[i][j] = (\n",
                "                    1 if j in self[i] else 0\n",
                "                )\n",
                "        return matrix",
            ],
        },
        {
            "cell_type": "code",
            "execution_count": 3,
            "metadata": {},
            "outputs": [
                {
                    "data": {"text/plain": ["2"]},
                    "execution_count": 3,
                    "metadata": {},
                    "output_type": "execute_result",
                }
            ],
            "source": [
                "g = Graph({0: {1, 2}, 1: {2, 0, 3}, 2: {0, 1, 3}, 3: {2, 1}})\n",
                "# square bisected (2 triangles)\n",
                "m = g.toAdjacencyMatrix()\n",
                "\n",
                "n = len(m)\n",
                "\n",
                "m2 = [[0] * n for _ in range(n)]\n",
                "\n",
                "for i in range(n):\n",
                "    for j in range(n):\n",
                "        for k in range(n):\n",
                "            m2[i][j] += m[i][k] * m[k][j]\n",
                "c = 0\n",
                "\n",
                "for i in range(n):\n",
                "    for j in range(n):\n",
                "        c += m[i][j] * m2[i][j]\n",
                "\n",
                "c // 6",
            ],
        },
        {
            "cell_type": "code",
            "execution_count": 4,
            "metadata": {},
            "outputs": [],
            "source": [
                "m = [ Felt(i) for i in flatten(m) ]\n",
                "m2 = [ Felt(i) for i in flatten(m2) ]",
            ],
        },
        {
            "cell_type": "code",
            "execution_count": 5,
            "metadata": {},
            "outputs": [],
            "source": [
                "prover = TriangleProver(m, m2, Felt(c))\n",
                "proof = prover.prove()\n",
                "\n",
                "verifier = TriangleVerifier(proof)\n",
                "verifier.verify()\n",
            ],
        },
